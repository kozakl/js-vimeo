{
  "name": "@kozakl/vimeo",
  "version": "1.1.2",
  "sideEffects": false,
  "author": {
    "name": "Luke Kozak",
    "email": "kozakluke@gmail.com",
    "url": "https://github.com/kozakl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kozakl/js-vimeo"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "publish-dist": "npm run build && cp package.json ./dist && npm publish ./dist --access=public",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watchAll"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/src/.*.test)\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  },
  "dependencies": {
    "@kozakl/utils": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^10.12.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^3.6.4"
  }
}
